{
  "name": "mauchede/rancher-api-bundle",
  "type": "library",
  "description": "Rancher API Bundle for the Symfony Framework",
  "keywords": [
    "api",
    "bundle",
    "client",
    "rancher"
  ],
  "homepage": "http://github.com/mauchede/rancher-api-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Morgan Auchede",
      "email": "morgan.auchede@gmail.com",
      "homepage": "http://github.com/mauchede"
    }
  ],
  "autoload": {
    "psr-4": {"Mauchede\\RancherApiBundle\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Mauchede\\RancherApiBundle\\Tests\\": "tests/"}
  },
  "require": {
    "php": ">=5.5",
    "mauchede/rancher-api": "1.0.x-dev",
    "symfony/framework-bundle": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
